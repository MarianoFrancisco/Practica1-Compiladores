<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        tools:context=".InsertarTexto">

    <EditText
            android:layout_width="match_parent"
            android:layout_height="478dp"
            android:inputType="textMultiLine"
            android:gravity="start|top"
            android:ems="10"
            android:id="@+id/textoUsuario" tools:ignore="MissingConstraints,SpeakableTextPresentCheck"/>
    <Button
            android:text="ProbarCupFlex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/cupFlex"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="488dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="124dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>